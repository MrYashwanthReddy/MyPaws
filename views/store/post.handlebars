{{#with data}}
  <form action="/pet-stores/review-post/{{{placeId}}}/{{{_id}}}" method="post">
    <input
      type="number"
      name="rating"
      placeholder="Enter Rating"
      min="1"
      max="5"
      value="{{{rating}}}"
    />
    <input
      type="text"
      name="comment"
      placeholder="Enter Comment"
      value="{{{comment}}}"
    />
    <input type="submit" />
  </form>
{{/with}}